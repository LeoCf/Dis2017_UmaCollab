<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="umaCollabApp.Views.Teams.TeamsViewPage"
             Title="Teams">

  <ScrollView>
    <StackLayout Padding="20">

      <Button x:Name="btnTeamRegister" Text ="Team Register" Command="{Binding TeamRegisterCommand}" ></Button>
      <Button x:Name="btnListAllTeams" Text ="List All Teams" Command="{Binding TeamListCommand}" ></Button>

    </StackLayout>
  </ScrollView>
</ContentPage>